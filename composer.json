{
	"name": "trejjam/mail-chimp",
	"description": "MailChimp client for Nette",
	"keywords": [
		"nette",
		"mailChimp"
	],
	"license": "MIT",
	"authors": [
		{
			"name": "Jan Trejbal"
		}
	],
	"require": {
		"php": ">=7.1",

		"nette/di": "~2.4",
		"trejjam/base-extension": "~0.7",
		"guzzlehttp/guzzle": "^6.2",
		"composer/ca-bundle": "^1.0",
		"tharos/schematic": "^1.2"
	},
	"require-dev": {
		"nette/tester": "^2.2",
		"nette/robot-loader": "^3.1",
		"nette/bootstrap": "^2.4",
		"tracy/tracy": "^2.4"
	},
	"autoload": {
		"psr-4": {
			"Trejjam\\MailChimp\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Trejjam\\MailChimp\\Tests\\": "tests/src/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "v0.2.x-dev"
		}
	}
}
