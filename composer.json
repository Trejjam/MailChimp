{
	"name": "trejjam/mail-chimp",
	"description": "MailChimp client for Nette",
	"keywords": [
		"nette",
		"mailChimp"
	],
	"license": "MIT",
	"authors": [
		{
			"name": "Jan Trejbal"
		}
	],
	"require": {
		"php": ">=7.1",

		"composer/ca-bundle": "^1.1",
		"nette/di": "^2.4.10 || ^3.0",
		"guzzlehttp/guzzle": "^6.3",
		"trejjam/base-extension": "^0.8",
		"tharos/schematic": "^1.2"
	},
	"require-dev": {
		"nette/robot-loader": "^3.1",
		"nette/bootstrap": "^2.4 || ^3.0",
		"tracy/tracy": "^2.4 || ^3.0",

		"nette/tester": "^2.2",

		"phpstan/phpstan": "^0.11",
		"phpstan/phpstan-deprecation-rules": "^0.11",
		"phpstan/phpstan-strict-rules": "^0.11",
		"thecodingmachine/phpstan-safe-rule": "^0.1",

		"symplify/easy-coding-standard": "^5.0",

		"sensiolabs/security-checker": "^5.0",
		"roave/security-advisories": "dev-master"
	},
	"autoload": {
		"psr-4": {
			"Trejjam\\MailChimp\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Trejjam\\MailChimp\\Tests\\": "tests/src/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "v0.2.x-dev"
		}
	}
}
